[global_config]
  always_split_with_profile = True
  suppress_multiple_term_dialog = True
[keybindings]
  toggle_zoom = <Alt>Return
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = massi
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    font = Noto Sans Mono Thin 12
    use_system_font = False
  [[massi]]
    cursor_color = "#aaaaaa"
    font = Monospace 11
    scrollback_infinite = True
    scrollbar_position = hidden
    show_titlebar = False
    use_system_font = False
